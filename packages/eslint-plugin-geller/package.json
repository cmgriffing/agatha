{
  "name": "eslint-plugin-geller",
  "version": "0.1.3",
  "description": "An ESLint wrapper around geller to identify undefined env vars",
  "repository": "https://github.com/cmgriffing/geller/packages/eslint-plugin-geller",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "geller",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "scripts": {
    "watch": "nodemon -e js,ts --watch lib --exec \"pnpm build\"",
    "build": "tsc",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "mocha tests --recursive",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "geller": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@typescript-eslint/types": "^5.57.1",
    "@typescript-eslint/utils": "^5.57.1",
    "eslint": "^8.19.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "ISC"
}
